{
  "require": {
    "craftcms/cms": "3.4.22.1",
    "craftcms/redactor": "2.6.1",
    "doublesecretagency/craft-siteswitcher": "2.1.0",
    "flipboxfactory/craft-link": "1.1.4",
    "jandh/translator": "1.0.1",
    "mikestecker/craft-videoembedder": "1.1.4",
    "nystudio107/craft-imageoptimize": "1.6.13",
    "nystudio107/craft-seomatic": "3.3.8",
    "superbig/craft3-templateselect": "2.0.2",
    "verbb/cp-nav": "3.0.12",
    "verbb/navigation": "1.3.19",
    "verbb/super-table": "2.5.0",
    "vlucas/phpdotenv": "^3.4.0",
    "wbrowar/adminbar": "3.1.8.1"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
